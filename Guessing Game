import random


def guessing_game():
    computers_number = random.randint(1, 100)
    i = 0
    while i < 7:
        try:
            players_number = int(input("Guess the number (1, 100):"))

            if players_number == computers_number:
                print("correct, Good Job")
                return
            elif players_number < computers_number:
                if i == 6:
                    print("You did not guess the number correctly.Better Luck Next Time")
                    return
                print("higher")
            elif players_number > computers_number:
                if i == 6:
                    print("You did not guess the number correctly.Better Luck Next Time")
                    return
                print("lower")

            i += 1

        except ValueError:
            print("That is not a number")


guessing_game()
